{
  "name": "@progress/kendo-drawing",
  "description": "Kendo UI Drawing",
  "version": "1.6.0",
  "main": "dist/npm/main.js",
  "module": "dist/es/main.js",
  "jsnext:main": "dist/es/main.js",
  "es2015": "dist/es2015/main.js",
  "typings": "dist/npm/main.d.ts",
  "jsdelivr": "dist/cdn/main.js",
  "unpkg": "dist/cdn/main.js",
  "scripts": {
    "api-check": "gulp api-check",
    "build-package": "gulp build-module && gulp build-cdn",
    "test": "gulp test && gulp test-typings",
    "e2e": "gulp e2e",
    "lint": "gulp lint",
    "verify-npm": "gulp verify-npm",
    "semantic-release": "semantic-release pre && semantic-prerelease publish && semantic-release post"
  },
  "keywords": [
    "Kendo UI",
    "Drawing",
    "Vector Graphics",
    "SVG",
    "Canvas"
  ],
  "dependencies": {
    "pako": "^1.0.5"
  },
  "devDependencies": {
    "@krux/condition-jenkins": "1.0.1",
    "@progress/kendo-angular-popup": "^1.0.2",
    "@progress/kendo-file-saver": "^1.0.0",
    "@progress/kendo-theme-default": "^2.0.0",
    "@progress/kendo-typescript-tasks": "^5.1.0",
    "@telerik/eslint-config": "^1.0.0",
    "@progress/kendo-package-tasks": "^3.0.6",
    "@telerik/semantic-prerelease": "^1.0.0",
    "@types/node": "^8.10.11",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "core-js": "^2.4.1",
    "cz-conventional-changelog": "^1.1.5",
    "ghooks": "^1.0.3",
    "gulp": "3.9.1",
    "gulp-typescript": "^3.2.2",
    "semantic-release": "^6.3.6",
    "typescript": "~2.5.0",
    "validate-commit-msg": "^1.1.1",
    "watchify": "^3.9.0"
  },
  "author": "Progress",
  "license": "See license in LICENSE.md",
  "@progress": {
    "friendlyName": "Drawing"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-drawing.git"
  },
  "bugs": {
    "url": "https://github.com/telerik/kendo-angular"
  },
  "release": {
    "debug": false,
    "branchTags": {
      "develop": "dev"
    },
    "fallbackTags": {
      "dev": "latest"
    },
    "analyzeCommits": "@telerik/semantic-prerelease/analyzeCommits",
    "generateNotes": "@telerik/semantic-prerelease/generateNotes",
    "getLastRelease": "@telerik/semantic-prerelease/getLastRelease",
    "verifyConditions": "@telerik/semantic-prerelease/verifyConditions",
    "verifyRelease": "@telerik/semantic-prerelease/verifyRelease"
  }
}